{
  "name": "homebridge-config-ui-x",
  "version": "4.4.4",
  "description": "Configuration UI plugin for Homebridge",
  "license": "MIT",
  "author": "oznu <dev@oz.nu>",
  "repository": {
    "type": "git",
    "url": "git://github.com/oznu/homebridge-config-ui-x.git"
  },
  "bugs": {
    "url": "https://github.com/oznu/homebridge-config-ui-x/issues"
  },
  "main": "./dist/index.js",
  "bin": {
    "homebridge-config-ui-x": "./dist/bin/standalone.js"
  },
  "engines": {
    "node": ">=8.15.1",
    "homebridge": ">=0.4.22"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "scripts": {
    "watch": "concurrently \"npm run watch:ui\" \"npm run watch:server\"",
    "watch:server": "nodemon",
    "watch:ui": "npm run start --prefix ui",
    "build": "npm run build:server && npm run build:ui",
    "build:server": "rimraf dist && tsc -p tsconfig.build.json && npm run webpack",
    "build:ui": "npm run build --prefix ui",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "npm run lint:server && npm run lint:ui",
    "lint:server": "tslint -p tsconfig.json -c tslint.json",
    "lint:ui": "npm run lint --prefix ui",
    "webpack": "webpack --config webpack.config.js",
    "install:ui": "npm install --prefix ui",
    "prepublishOnly": "npm i --package-lock-only && npm audit && npm run lint && npm run build"
  },
  "bundledDependencies": [
    "@nestjs/common",
    "@nestjs/core",
    "@nestjs/platform-fastify",
    "@nestjs/platform-socket.io",
    "@nestjs/websockets",
    "class-transformer",
    "class-validator",
    "commander",
    "fastify",
    "fastify-static",
    "reflect-metadata",
    "rxjs",
    "semver"
  ],
  "dependencies": {
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/platform-fastify": "^6.2.4",
    "@nestjs/platform-socket.io": "^6.2.4",
    "@nestjs/websockets": "^6.2.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "commander": "^2.20.0",
    "fastify": "^2.4.1",
    "fastify-static": "^2.4.0",
    "node-pty-prebuilt-multiarch": "0.9.0-beta2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.2",
    "semver": "^6.1.1"
  },
  "devDependencies": {
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@oznu/hap-client": "0.0.4",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^12.0.3",
    "@types/socket.io": "^2.1.2",
    "bash-color": "0.0.4",
    "buffer-shims": "^1.0.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "fs-extra": "^8.0.1",
    "helmet": "^3.18.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pino-pretty": "^3.1.0",
    "point-of-view": "^3.5.0",
    "prettier": "^1.17.1",
    "qr-image": "^3.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  }
}